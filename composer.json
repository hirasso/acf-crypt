{
  "name": "hirasso/acf-encryption",
  "description": "Protect sensitive data in your ACF fields using encryption ðŸ”",
  "type": "wordpress-muplugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Rasso Hilber",
      "email": "mail@rassohilber.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Hirasso\\ACFEncrypt\\": "src/"
    }
  },
  "require": {
    "php": ">=8.2",
    "defuse/php-encryption": "^2.4"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.1",
    "friendsofphp/php-cs-fixer": "^3.59",
    "brianhenryie/strauss": "^0.19.2"
  },
  "config": {},
  "scripts": {
    "prefix-namespaces": [
      "strauss",
      "composer dump-autoload"
    ],
    "post-install-cmd": [
      "@prefix-namespaces"
    ],
    "post-update-cmd": [
      "@prefix-namespaces"
    ]
  },
  "extra": {
    "strauss": {
        "target_directory": "strauss"
    }
  }
}
